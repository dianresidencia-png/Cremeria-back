{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productoNombre": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "productoNombre"
    },
    "descripcion": {
      "type": "text"
    },
    "imagenes": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "precio": {
      "type": "decimal"
    },
    "fechacreacion": {
      "type": "date"
    },
    "pesokg": {
      "type": "decimal"
    },
    "destacado": {
      "type": "boolean",
      "default": true
    },
    "disponible": {
      "type": "boolean",
      "default": true
    },
    "categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria",
      "inversedBy": "productos"
    },
    "marca": {
      "type": "enumeration",
      "enum": [
        "Higienica",
        "Regionales",
        "Otras"
      ]
    }
  }
}
